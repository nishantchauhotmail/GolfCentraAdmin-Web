@section scripts{
    <script src="~/Scripts/Custom/TeeSheetController.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            jQuery('#txtdate').datepicker({
                autoclose: true,
                todayHighlight: true,
                toggleActive: true,
                startDate: new Date(),

            }).datepicker("setDate", "0");
            OnTeeSheetDetailReady();
          
        });
    </script>
}
