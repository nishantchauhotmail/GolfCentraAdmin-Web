@model List<GolfCentraAdmin.ViewModel.TeeSheetViewModel>
<div class="col-lg-12">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">
                @*  Member*@
            </h4>

            <div class="table-responsive m-t-40">
                <table id="myTable" class="table table-bordered table-striped table-hover">
                    <thead>
                        <tr>
                            <th>Tee Time</th>
                            <th>Course</th>
                            <th>Player 1</th>
                            <th>Player 2</th>
                            <th>Player 3</th>
                            <th>Player 4</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.TeeTime </td>
                                <td>@item.CourseName</td>
                                <td>@item.Player1</td>
                                <td>@item.Player2</td>
                                <td>@item.Player3</td>
                                <td>@item.Player4</td>
                                <td>@item.Status</td>
                            
                                <td> <div class="label label-table label-success"  onclick="OnBookBookingButtonClick(@item.SessionSlotId,@item.CoursePairingId,@item.SessionId)">Book</div></td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {

        $('#myTable').DataTable({
            dom: 'Bfrtip',
            buttons: [
                'copy', 'csv', 'excel'
            ]
        });
    });
</script>
