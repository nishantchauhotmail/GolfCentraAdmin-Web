
@{
    ViewBag.Title = "Error";
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="@GolfCentraAdmin.UI.Common.Constants.Url.WebSiteUrl/Theme/images/favicon.png">
    <title>@GolfCentraAdmin.UI.Common.Constants.Name.AppName</title>
    <!-- Bootstrap Core CSS -->
    @Styles.Render("~/Content/css")
    <link href="~/Theme/css/pages/login-register-lock.css" rel="stylesheet">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style>
        body {
            background-image: url(data:image/png;base64,);
        }

        .error-template {
            padding: 40px 15px;
            text-align: center;
        }

        .error-actions {
            margin-top: 15px;
            margin-bottom: 15px;
        }

            .error-actions .btn {
                margin-right: 10px;
            }
    </style>
</head>

<body class="card-no-border">
    <div id="divDomain" style="display: none">@GolfCentraAdmin.UI.Common.Constants.Url.WebSiteUrl</div>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label">@GolfCentraAdmin.UI.Common.Constants.Name.AppName</p>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <section id="wrapper" class="fc-center">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="error-template">
                        <h1>
                            Oops!
                        </h1>
                        <h2></h2>
                        <div class="error-details">
                            Sorry, an error has occured, Requested page not found!
                        </div>
                        <div class="error-actions">
                            <a href="@GolfCentraAdmin.UI.Common.Constants.Url.WebSiteUrlWithoutSlash/Dashboard/Index" class="btn btn-primary btn-lg">
                                <span class="glyphicon glyphicon-home"></span>
                                Take Me Home
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </section>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    @Scripts.Render("~/bundles/js")
    <!--Custom JavaScript -->

    <script type="text/javascript">
            $(function () {
                $(".preloader").fadeOut();
            });
        
            // ==============================================================
            // Login and Recover Password
            // ==============================================================

    </script>

</body>

</html>