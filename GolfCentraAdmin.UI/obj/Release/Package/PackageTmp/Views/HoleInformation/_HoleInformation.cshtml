@model GolfCentraAdmin.ViewModel.HoleViewModel




    @Html.HiddenFor(x => x.HoleNumberId)
    @Html.HiddenFor(x => x.HoleInformationId)
    @{int count = 0;}
    @foreach (var item in Model.TeeViewList)
    {
        @Html.HiddenFor(x => x.TeeViewList[count].HoleTeeYardageId)
        @Html.HiddenFor(x => x.TeeViewList[count].TeeId)
        <div class="form-group row">
            <label for="inputEmail3" class="col-sm-3 text-right control-label col-form-label">@item.TeeName</label>
            <div class="col-sm-9">
                @Html.TextBoxFor(x => x.TeeViewList[count].Yardage, new { @id = "txtYardage", @autocomplete = "off", @class = "form-control", @placeholder = "Yards" })
            </div>
        </div>
        count++;
    }

    <div class="form-group row">
        <label for="inputEmail3" class="col-sm-3 text-right control-label col-form-label">Hole End Point</label>
        <div class="col-sm-5">
            @Html.TextBoxFor(x => x.Latitude, new { @id = "txtLatitude", @autocomplete = "off", @class = "form-control", @placeholder = "Latitude" })
        </div>
        <div class="col-sm-4">
            @Html.TextBoxFor(x => x.Longitude, new { @id = "txtLongitude", @autocomplete = "off", @class = "form-control", @placeholder = "Longitude" })
        </div>
    </div>
    <div class="form-group m-b-0">
        <div class="offset-sm-3 col-sm-9">
            <button type="submit" class="btn btn-info waves-effect waves-light m-t-10" id="btnSubmit">Save</button>
            <div id="divMessage"></div>
        </div>
    </div>
