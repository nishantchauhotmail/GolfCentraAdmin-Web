@model List<GolfCentraAdmin.ViewModel.SessionMasterViewModel>
<!-- ============================================================== -->
<!-- Bread crumb and right sidebar toggle -->
<!-- ============================================================== -->
<div class="row page-titles">
    <div class="col-md-5 align-self-center">
        <h3 class="text-themecolor">Session Master</h3>

    </div>
    <div class="col-md-7 align-self-center text-right d-md-block">
      @*  <a href="" data-toggle="modal" data-target=".bs-example-modal-lg"> <button type="button" class="btn btn-info"><i class="fa fa-plus-circle"></i> Session Price</button></a>*@
        <a href="@GolfCentraAdmin.UI.Common.Constants.Url.WebSiteUrlWithoutSlash/SessionDetail/Index"> <button type="button" class="btn btn-info"><i class="fa fa-plus-circle"></i> Manage Session</button></a>
    </div>

</div>
<!-- ============================================================== -->
<!-- End Bread crumb and right sidebar toggle -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- Start Page Content -->
<!-- ============================================================== -->
<!-- Row -->
<div class="row">

    <!-- Column -->
    <div class="col-lg-12 col-xlg-9 col-md-12">
        <div class="card">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs profile-tab" role="tablist">
                <li class="nav-item" onclick="OnTabChange(1)"> <a class="nav-link active" data-toggle="tab" href="#home" role="tab">Book Tee Time</a> </li>
                <li class="nav-item" onclick="OnTabChange(2)"> <a class="nav-link" data-toggle="tab" href="#profile" role="tab">Book Driving Range</a> </li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <div class="tab-pane active" id="home" role="tabpanel">
                    <div class="col-lg-12 col-md-12">
                        <div class="card">
                            <div class="card-body">
                                <h2>Book Tee Time</h2>
                                <span id="divBookingTab"></span>

                            </div>
                        </div>
                    </div>
                </div>
                <!--second tab-->
                <div class="tab-pane" id="profile" role="tabpanel">
                    <div class="col-lg-12 col-md-12">
                        <div class="card">
                            <div class="card-body">
                                <h2>Book Driving Range</h2>
                                <span id="divDrivingRangeTab"></span>

                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
    <!-- Column -->
</div>
<!-- Row -->
<!-- ============================================================== -->
<!-- End PAge Content -->
<!-- ============================================================== -->
<!-- sample modal content -->

<!-- /.modal -->


@section scripts{
    <script src="~/Scripts/Custom/SessionMasterController.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            OnSessionMasterPageReady();
        });
    </script>
}
